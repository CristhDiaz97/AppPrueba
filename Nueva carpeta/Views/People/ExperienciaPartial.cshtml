                @Html.DisplayFor(modelItem => item.FchInicio)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FchFin)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Descripcion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Estado)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FchCreate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FchUpdate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdPersonaNavigation.IdPersona)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdExperienciaLaboral">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdExperienciaLaboral">Details</a> |
