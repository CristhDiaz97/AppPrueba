@model IEnumerable<WebEmpleo.Models.Experiencia>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.FchInicio)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FchFin)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Descripcion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Estado)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FchCreate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FchUpdate)
